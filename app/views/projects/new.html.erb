<h2>新しいプロジェクトを作成</h2>

<%= form_with model: @project, url: projects_path, local: true do |f| %>
  <div class="mb-3">
    <%= f.label :project_name, "プロジェクト名" %>
    <%= f.text_field :project_name, class: "form-control", required: true %>
  </div>

  <div class="mb-3">
    <%= f.label :description, "説明" %>
    <%= f.text_area :description, class: "form-control" %>
  </div>

  <div>
    <%= f.submit "作成", class: "btn btn-primary" %>
  </div>
<% end %>
